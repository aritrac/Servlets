<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>JSP XML</display-name>
  <welcome-file-list>
    <welcome-file>JSPs/login.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>DispatcherServlet</servlet-name>
    <servlet-class>com.fog.computing.servlet.DispatcherServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DispatcherServlet</servlet-name>
    <url-pattern>/Dispatcher</url-pattern>
  </servlet-mapping>
  <context-param>
    <param-name>dbuser</param-name>
    <param-value>aritra as SYSDBA</param-value>
  </context-param>
  <context-param>
    <param-name>dbpassword</param-name>
    <param-value>oradba123</param-value>
  </context-param>
  <context-param>
    <param-name>dbURL</param-name>
    <param-value>localhost:1521:xe</param-value>
  </context-param>
  <filter>
    <filter-name>SessionFilter</filter-name>
    <filter-class>com.fog.computing.servlet.SessionFilter</filter-class>
</filter>
<filter-mapping>
    <filter-name>SessionFilter</filter-name>
    <url-pattern>/JSPs/user_home_page.jsp</url-pattern>
</filter-mapping>
<filter-mapping>
    <filter-name>SessionFilter</filter-name>
    <url-pattern>/JSPs/admin_home_page.jsp</url-pattern>
</filter-mapping>
<filter-mapping>
    <filter-name>SessionFilter</filter-name>
    <url-pattern>/JSPs/edit_user.jsp</url-pattern>
</filter-mapping>
<filter-mapping>
    <filter-name>SessionFilter</filter-name>
    <url-pattern>/JSPs/file_details_page.jsp</url-pattern>
</filter-mapping>
<filter-mapping>
    <filter-name>SessionFilter</filter-name>
    <url-pattern>/JSPs/file_upload_page.jsp</url-pattern>
</filter-mapping>
<session-config>
    <session-timeout>10</session-timeout>
</session-config>
</web-app>